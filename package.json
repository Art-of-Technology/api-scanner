{
  "name": "api-scanner",
  "version": "1.0.3",
  "description": "Next.js API route scanner that automatically generates API documentation",
  "main": "dist/index.js",
  "bin": {
    "api-scanner": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "cp -r src/templates dist/",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "lint": "echo 'No linting configured'",
    "format": "echo 'No formatting configured'"
  },
  "keywords": [
    "nextjs",
    "api",
    "scanner",
    "documentation",
    "swagger",
    "openapi",
    "typescript"
  ],
  "author": "AOT Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "typescript": "^5.3.3",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "fs-extra": "^11.2.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/fs-extra": "^11.0.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
