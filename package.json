{
  "name": "api-scanner",
  "version": "1.0.0",
  "description": "Next.js API route scanner that automatically generates API documentation",
  "main": "dist/index.js",
  "bin": {
    "api-scanner": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "nextjs",
    "api",
    "scanner",
    "documentation",
    "swagger",
    "openapi",
    "typescript"
  ],
  "author": "AOT Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "typescript": "^5.3.3",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ]
}
